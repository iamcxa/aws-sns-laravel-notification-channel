{
    "name": "laravel-notification-channels/aws-sns",
    "description": "AWS SNS channel for Laravel Notifications",
    "homepage": "https://github.com/laravel-notification-channels/aws-sns",
    "license": "MIT",
    "authors": [
        {
            "name": "Menaf Gul",
            "email": "menaf@menafgul.com",
            "homepage": "http://www.menafgul.com",
            "role": "Developer"
        }
    ],
    "require": {
        "aws/aws-sdk-php-laravel": "~3.0"
        "php": ">=7.2",
        "illuminate/notifications": "~5.5 || ~6.0 || ~7.0 || ~8.0",
        "illuminate/support": "~5.5 || ~6.0 || ~7.0 || ~8.0",
        "illuminate/queue": "~5.5 || ~6.0 || ~7.0 || ~8.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\AwsSns\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\AwsSns\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
